{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "todo-list",
  "version": "0.1.7",
  "identifier": "com.todo-list.app",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "todo-list",
        "width": 525,
        "height": 600,
        "decorations": false,
        "transparent": true,
        "shadow": false,
        "center": true,
        "resizable": false,
        "fullscreen": false,
        "alwaysOnTop": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "msi",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.ico"
    ],
    "resources": []
  }
}
